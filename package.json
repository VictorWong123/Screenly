{
  "name": "screenly",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:website": "cd apps/website && npm run dev",
    "build:website": "cd apps/website && npm run build",
    "build:extension": "cd apps/extension && npm run build",
